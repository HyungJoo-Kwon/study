      "source": [
        "if '__file__' in globals():\n",
        "    import os, sys\n",
        "    sys.path.append(os.path.join(os.path.dirname(__file__), '..'))\n",
        "import numpy as np\n",
        "from dezero import Variable\n",
        "\n",
        "x0 = Variable(np.array([1, 2, 3]))\n",
        "x1 = Variable(np.array([10]))\n",
        "y = x0 + x1\n",
        "print(y)\n",
        "\n",
        "y.backward()\n",
        "print(x1.grad)"
      ],
